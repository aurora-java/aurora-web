<?xml version="1.0" encoding="UTF-8"?>
<ns1:model xmlns:ns1="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="f" baseTable="sys_function_service">
    <ns1:fields>
        <ns1:field name="function_id" databaseType="BIGINT" datatype="java.lang.Long"/>
        <ns1:field name="service_id" databaseType="BIGINT" datatype="java.lang.Long"/>
    </ns1:fields>
    <ns1:primary-key>
        <ns1:pk-field name="function_id"/>
        <ns1:pk-field name="service_id"/>
    </ns1:primary-key>
    <ns1:relations>
        <ns1:relation name="ss" joinType="LEFT OUTER" refModel="sys.sys_service">
            <ns1:reference foreignField="service_id" localField="service_id"/>
        </ns1:relation>
    </ns1:relations>
    <ns1:query-fields>
        <ns1:query-field field="function_id" queryOperator="="/>
    </ns1:query-fields>
    <ns1:features>
        <f:standard-who/>
    </ns1:features>
    <ns1:ref-fields>
        <ns1:ref-field name="service_name" prompt="PAGE" relationName="ss" sourceField="service_name"/>
        <ns1:ref-field name="title" prompt="EXP_BUSINESS_APPR_NODE.TITLE" relationName="ss" sourceField="title"/>
    </ns1:ref-fields>
</ns1:model>
