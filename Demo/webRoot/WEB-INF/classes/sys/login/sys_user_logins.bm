        <bm:field name="logout_time" databaseType="DATE" datatype="java.util.Date" physicalName="LOGOUT_TIME" prompt="SYS_USER_LOGINS.LOGOUT_TIME"/>
        <bm:field name="last_active_time" databaseType="DATE" datatype="java.util.Date" insertExpression="now()" physicalName="LAST_ACTIVE_TIME" prompt="SYS_USER_LOGINS.LAST_ACTIVE_TIME"/>
        <bm:field name="description" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="DESCRIPTION" prompt="SYS_USER_LOGINS.DESCRIPTION"/>
