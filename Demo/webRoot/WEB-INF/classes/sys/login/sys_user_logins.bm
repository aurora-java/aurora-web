<bm:model xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="SYS_USER_LOGINS">
    <bm:fields>
        <bm:field name="login_id" databaseType="NUMBER" datatype="java.lang.Long" isAutoGeneratedKey="true" physicalName="LOGIN_ID" prompt="SYS_USER_LOGINS.LOGIN_ID"/>
        <bm:field name="user_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="USER_ID" prompt="SYS_USER_LOGINS.USER_ID"/>
        <bm:field name="role_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="ROLE_ID" prompt="SYS_USER_LOGINS.ROLE_ID"/>
        <bm:field name="company_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="COMPANY_ID" prompt="SYS_USER_LOGINS.COMPANY_ID"/>
        <bm:field name="session_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SESSION_ID" prompt="SYS_USER_LOGINS.SESSION_ID"/>
        <bm:field name="encrypted_session_id" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ENCRYPTED_SESSION_ID" prompt="SYS_USER_LOGINS.ENCRYPTED_SESSION_ID"/>
        <bm:field name="nls_language" databaseType="VARCHAR2" datatype="java.lang.String" insertExpression="${/session/@lang}" physicalName="NLS_LANGUAGE" prompt="SYS_USER_LOGINS.NLS_LANGUAGE"/>
        <bm:field name="machine_serial" databaseType="VARCHAR2" datatype="java.lang.String" insertExpression="&apos;UNDIFINE&apos;" physicalName="MACHINE_SERIAL" prompt="SYS_USER_LOGINS.MACHINE_SERIAL"/>
        <bm:field name="client_ip_address" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CLIENT_IP_ADDRESS" prompt="SYS_USER_LOGINS.CLIENT_IP_ADDRESS"/>
